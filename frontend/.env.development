# Development environment configuration
# React environment variables must start with REACT_APP_

# Backend API URL for development
REACT_APP_API_URL=http://localhost:8000

# Enable additional logging in development
REACT_APP_DEBUG=true

# Suppress webpack dev server deprecation warnings
# These warnings are from react-scripts 5.0.1 using deprecated webpack-dev-server middleware options
# Disabling sourcemaps reduces webpack console noise during development
GENERATE_SOURCEMAP=false

# Disable Node.js deprecation warnings in development
# This will suppress the onAfterSetupMiddleware and onBeforeSetupMiddleware warnings
NODE_OPTIONS=--no-deprecation